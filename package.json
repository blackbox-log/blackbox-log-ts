{
	"$schema": "https://json.schemastore.org/package.json",
	"name": "blackbox-log",
	"version": "0.2.1",
	"license": "GPL-3.0-only",
	"description": "Parser for Betaflight & INAV blackbox logs",
	"keywords": [
		"blackbox-log",
		"betaflight",
		"inav"
	],
	"homepage": "https://blackbox-log.github.io/blackbox-log-ts/",
	"repository": "github:blackbox-log/blackbox-log-ts",
	"type": "module",
	"files": [
		"dist",
		"src"
	],
	"exports": {
		".": {
			"types": "./dist/entry/main.d.ts",
			"default": "./dist/main.js"
		},
		"./slim": {
			"types": "./dist/entry/slim.d.ts",
			"default": "./dist/slim.js"
		},
		"./wasm": "./dist/blackbox-log.wasm"
	},
	"dependencies": {
		"typescript-memoize": "^1.1.1"
	},
	"devDependencies": {
		"@typescript-eslint/eslint-plugin": "^6.0.0",
		"@typescript-eslint/parser": "^6.0.0",
		"eslint": "^8.38.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-config-xo": "^0.43.1",
		"eslint-config-xo-typescript": "^1.0.0",
		"eslint-import-resolver-typescript": "^3.5.5",
		"eslint-plugin-import": "npm:eslint-plugin-i@2.27.5-4",
		"eslint-plugin-tsdoc": "^0.2.17",
		"eslint-plugin-unicorn": "^47.0.0",
		"prettier": "^3.0.0",
		"typedoc": "^0.24.4",
		"typescript": "^5.0.4",
		"vite": "^4.2.1",
		"vite-plugin-static-copy": "^0.17.0"
	},
	"packageManager": "pnpm@8.6.8"
}
